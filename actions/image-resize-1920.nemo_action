[Nemo Action]
Active=true
Name=Resize 1920px
Comment=Resize image to 1920px using ImageMagick's
Exec=bash -c "mkdir -p %P/resized; convert %F -resize '1920>' -set filename: %P/resized/'%t_1920.%e' '%[filename:]'"
EscapeSpaces=false
Separator=,
Icon-Name=sound
Selection=notnone
Mimetypes=image/*;
Terminal=false
Dependencies=mogrify;
